{
  "expo": {
    "name": "smart-home-panel",
    "slug": "smart-home-panel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://www.flaticon.com/free-icons/smart-home",
    "scheme": "smarthomepanel",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://www.flaticon.com/free-icons/smart-home",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "https://www.flaticon.com/free-icons/smart-home"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://www.flaticon.com/free-icons/smart-home",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
